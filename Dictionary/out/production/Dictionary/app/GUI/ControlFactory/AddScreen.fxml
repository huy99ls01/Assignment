      <Label layoutX="224.0" layoutY="41.0" text="Add a new word to dictionary" />
      <GridPane layoutX="75.0" layoutY="84.0" prefHeight="231.0" prefWidth="442.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
